<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>chilternquizleague</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description>JAX-RS Tools Generated - Do not modify</description>
    <display-name>Entity REST Services</display-name>
    <servlet-name>EntityServices</servlet-name>
    <servlet-class>org.chilternquizleague.web.EntityService</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>EntityServices</servlet-name>
    <url-pattern>/entity/*</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>org.chilternquizleague.web.EntityRegistrationListener</listener-class>
  </listener>
  <listener>
    <listener-class>org.chilternquizleague.web.ApplicationStartListener</listener-class>
  </listener>
  <filter>
    <filter-name>ObjectifyFilter</filter-name>
    <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ObjectifyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>PassThroughFilter</filter-name>
    <filter-class>org.chilternquizleague.web.PassThroughFilter</filter-class>
  </filter>
  <filter>
    <filter-name>MaintainURLRewriteFilter</filter-name>
    <filter-class>org.chilternquizleague.web.MaintainURLRewriteFilter</filter-class>
  </filter>
  <filter>
    <filter-name>URLRewriteFilter</filter-name>
    <filter-class>org.chilternquizleague.web.URLRewriteFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>PassThroughFilter</filter-name>
    <url-pattern>/_ah/*</url-pattern>
    <url-pattern>/tasks/*</url-pattern>
    <url-pattern>/entity/*</url-pattern>
    <url-pattern>/view/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <filter-mapping>
    <filter-name>MaintainURLRewriteFilter</filter-name>
    <url-pattern>/maintain/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <filter-mapping>
    <filter-name>URLRewriteFilter</filter-name>
    <url-pattern>/*</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>View REST Services</display-name>
    <servlet-name>ViewServices</servlet-name>
    <servlet-class>org.chilternquizleague.web.ViewService</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewServices</servlet-name>
    <url-pattern>/view/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>mailhandler</servlet-name>
    <servlet-class>org.chilternquizleague.web.MailHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>mailhandler</servlet-name>
    <url-pattern>/_ah/mail/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>statshandler</servlet-name>
    <servlet-class>org.chilternquizleague.web.StatsQueueHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>statshandler</servlet-name>
    <url-pattern>/tasks/stats</url-pattern>
  </servlet-mapping>
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>maintainance</web-resource-name>
      <url-pattern>/maintain/*</url-pattern>
      <url-pattern>/entity/*</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>admin</role-name>
    </auth-constraint>
    <user-data-constraint>
      <transport-guarantee>CONFIDENTIAL</transport-guarantee>
    </user-data-constraint>
  </security-constraint>
</web-app>